{
    "name": "tarioch/eveapi-fetcher-bundle",
    "description": "Provides automatic EVE Online Api fetching to your Symfony2 project.",
    "keywords": ["EVE Online", "EVE API", "Symfony"],
    "homepage": "https://github.com/tarioch/eveapi-fetcher-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
		"name": "Talos Katuma",
		"homepage": "https://hplcorp.net"
        }
    ],
    "support": {
	"issues": "https://github.com/tarioch/eveapi-fetcher-bundle/issues"
    },
    "require": {
        "php": ">=5.4.4",
	"symfony/framework-bundle": "2.3.*",
	"jms/di-extra-bundle" : "1.4.*",
	"tarioch/pheal-bundle" : "0.1.*"
    },
    "require-dev": {
	"phpunit/phpunit" : "3.7.*",
	"mockery/mockery" : "dev-master@dev"
    },
    "autoload": {
	"psr-0": { "Tarioch\\EveapiFetcherBundle": "" }
    },
    "target-dir": "Tarioch/EveapiFetcherBundle"
}
